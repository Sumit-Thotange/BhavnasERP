
@{
    ViewBag.Title = "FranchiseOrderIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Content/Project-JS/Product/Product.js"></script>
<script src="~/Content/Project-JS/Franchise/FranchiseOrder.js"></script>
<style>
    .dataTables_filter {
        margin-bottom: 10px;
    }
</style>
<div class="card" style="margin-bottom:5px;">
    <div class="card-body">
        <table style="margin-top:5px;">
            <tr>
                <td>
                    <span><h3><b>Franchise Order </b></h3></span>
                </td>

            </tr>
            <tr>
                <td>

                    <b>Franchise : </b><span>   Sankalp Bakery  (Prajwal Gode)</span>
                </td>
            </tr>
        </table>
    </div>
</div>
<section>
    <div>
        <div class="card p-4">

            <div class="row my-2 mt-1">
                <div class="col-md-12 text-end">
                    <a type="button" href="/Franchise/FranchiseIndex" class="btn btn-danger">Back</a>
                    <a class="btn btn-primary" href="/FranchiseProduct/FranchiseProductIndex"><i class="bi bi-cart"></i> &nbsp;Add Order</a>
                </div>
            </div>

            <div class="table-responsive">
                <table id="tblFrenOrder" class="table table-bordered table-hover" style="width:100%">
                    <thead class="text-white bg-dark">
                        <tr>
                            <td class="text-white bg-dark">OrderDate</td>
                            <td class="text-white bg-dark"> TotalAmount</td>
                            <td class="text-white bg-dark"> PaidAmount</td>
                            <td class="text-white bg-dark"> DeliveryDate</td>
                            <td class="text-white bg-dark"> Address</td>
                            <td class="text-white bg-dark"> </td>

                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>21/02/23</td>
                            <td>5000</td>
                            <td>12000</td>
                            <td>21/02/23</td>
                            <td>Balaji Nagar</td>
                            <td>@*<button type='button' class='btn btn-primary btn-sm' onclick='Account(" + elementvalue.Id + ")'><i class='bi bi-cart'></i></button>*@&nbsp;<button type='button' class='btn btn-success btn-sm' onclick='FranchiseEdit(" + elementvalue.Id + ")'><i class='bi bi-pencil-square' aria-hidden='true'></i></button>&nbsp;<button type='button' class='btn btn-info btn-sm' onclick='franchiseDetails(" + elementvalue.Id + ")'><i class='bi bi-eye-fill'></i></button>&nbsp;<button type='button' class='btn btn-danger btn-sm' onclick='DeleteOrganization(" + elementvalue.Id + ")'><i class='bi bi-trash-fill'></i></button></td>

                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>

<div id="Placeorder" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">

            <div class="modal-body">
                <h5 class="card-title">Franchise Order</h5>

                <!-- Floating Labels Form -->
                <form class="row g-3">
                    <div class="col-md-4">
                        <div class="form-floating">
                            <input type="date" class="form-control" id="floatingName" placeholder="OrderDate">
                            <label for="floatingDate">OrderDate</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-floating">
                            <input type="date" class="form-control" id="floatingDate" placeholder="Delivery Date">
                            <label for="floatingDeliveryBy">Delivery Date</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="DeliveryBy">
                            <label for="floatingPassword">Delivery By</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="DeliveryPerson">
                            <label for="floatingDeliveryPerson">Delivery Person</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="col-md-12">
                            <div class="form-floating">
                                <input type="text" class="form-control" id="DeliveryContact" placeholder="DeliveryContact">
                                <label for="floatingDeliveryContact">DeliveryContact</label>
                            </div>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="form-floating">
                            <textarea class="form-control" placeholder="Address" id="floatingTextarea" style="height: 100px;"></textarea>
                            <label for="floatingTextarea">Address</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="City">
                            <label for="floatingCity">City</label>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-floating mb-3">
                            <select class="form-select" id="floatingSelect" aria-label="State">
                                <option selected>Maharashtra</option>
                                <option value="1">UP</option>
                                <option value="2">Chattisgarh</option>
                            </select>
                            <label for="floatingSelect">State</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="Country">
                            <label for="floatingCountry">Country</label>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="floatingZip" placeholder="Zip">
                            <label for="floatingZip">Zip</label>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-floating">
                            <select class="form-control" id="txtPaymentMode">
                                <option value="">Select Payment Method</option>
                                <option value="Cash">Cash</option>
                                <option value="Cheque">Cheque</option>
                                <option value="DD">DD</option>
                                <option value="NEFT">NEFT</option>
                                <option value="RTGS">RTGS</option>
                                <option value="IMPS">IMPS</option>
                                <option value="UPI">UPI</option>
                                <option value="Net Banking">Net Banking</option>
                                <option value="Debit Card">Debit Card</option>
                                <option value="Credit Card">Credit Card</option>
                                <option value="PhonePe">PhonePe</option>
                                <option value="GooglePay">GooglePay</option>
                                <option value="Paytm">Paytm</option>
                                <option value="Paypal">Paypal</option>
                            </select>
                            <label for="PaidAmount">Payment Mode</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="Amount">
                            <label for="floatingAmount">Amount</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="DeliveryAmount">
                            <label for="floatingDeliveryAmount">Delivery Amount</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="TotalAmount">
                            <label for="floatingTotalAmount">Total Amount</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="Discount">
                            <label for="floatingDiscount">Discount</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="SGST">
                            <label for="floatingSGST ">SGST</label>
                        </div>
                    </div>

                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="CGST">
                            <label for="floatingCGST">CGST</label>
                        </div>
                    </div>

                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="IGST">
                            <label for="floatingIGST">IGST</label>
                        </div>
                    </div>

                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="Discount">
                            <label for="floatingDiscount">Discount</label>
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="form-floating">
                            <input type="text" class="form-control" id="#" placeholder="TotallyPayableAmount">
                            <label for="floatingTotallyPayableAmount">TotallyPayableAmount</label>
                        </div>
                    </div>

                    <div class="text-center">
                        <button type="submit" class="btn btn-primary">Submit</button>
                        <button type="reset" class="btn btn-secondary">Reset</button>
                    </div>
                </form><!-- End floating Labels Form -->

            </div>
        </div>

    </div>
</div>

<link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css'>
<link rel="stylesheet" href="https://cdn.datatables.net/1.13.6/css/jquery.dataTables.css" />
<script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.js"></script>
<script>
    var js = jQuery.noConflict(true);
    js(document).ready(function () {
        js('#tblFrenOrder').DataTable();

    });
</script>
